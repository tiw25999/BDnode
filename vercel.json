{
  "version": 2,
  "functions": {
    "api/products.js": {
      "runtime": "nodejs18.x"
    },
    "api/auth.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/products/(.*)",
      "destination": "/api/products"
    },
    {
      "source": "/products/(.*)",
      "destination": "/api/products"
    },
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/auth"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
