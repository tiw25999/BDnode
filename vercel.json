{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "source": "/products/(.*)",
      "destination": "/dist/server.js"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/dist/server.js"
    },
    {
      "source": "/cart/(.*)",
      "destination": "/dist/server.js"
    },
    {
      "source": "/orders/(.*)",
      "destination": "/dist/server.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/dist/server.js"
    },
    {
      "source": "/health",
      "destination": "/dist/server.js"
    },
    {
      "source": "/",
      "destination": "/dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
